#!/bin/bash -e

#useful=$(awk '/Useful commands/,/Automatic partition tool/{print}' < ~/dotfiles/readme.md)
#conf=$(awk '/Extra configuration tool/,/Environment variables/{print}' < ~/dotfiles/readme.md)
find ~/dotfiles/bin -type f | sort | while read line; do
    grep -e '`'"$(basename $line)"'\s*`' \
        < ~/dotfiles/readme.md 1>/dev/null || \
        echo "$(basename $line)"
done

